{
  "_id": "main",
  "template": "builtin:template",
  "content": [
    {
      "format": "h1",
      "field": {
        "type": "text",
        "id": "_id"
      }
    },
    {
      "format": "p",
      "field": {
        "type": "text",
        "id": "label"
      }
    },
    {
      "format": "p",
      "cells": [
        {
          "field": {
            "type": "text",
            "id": "remote_url"
          }
        },
        {
          "field": {
            "type": "option",
            "id": "location",
            "options": ["local only", "remote only", "local and remote"]
          }
        }
      ]
    }
  ]
}

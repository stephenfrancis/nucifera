{
  "_id": "main",
  "template": "builtin:template",
  "description": "Default template for a database's data documents",
  "content": [
    {
      "format": "h1",
      "field": {
        "type": "text",
        "id": "title"
      }
    },
    {
      "format": "p",
      "justifyContent": "space-between",
      "cells": [
        {
          "field": {
            "type": "date",
            "id": "created_at",
            "hidden_for_edit": true
          }
        },
        {
          "field": {
            "type": "text",
            "id": "created_by",
            "hidden_for_edit": true
          }
        }
      ]
    },
    {
      "format": "p",
      "field": {
        "type": "richtext",
        "id": "body"
      }
    }
  ]
}

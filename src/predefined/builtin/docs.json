{
  "_id": "docs",
  "template": "builtin:view",
  "description": "Default view of a database's data documents",
  "type": "list",
  "index": ["template"],
  "selector": {
    "template": {
      "$regex": "^(?!builtin:.*$).*"
    }
  },
  "columns": [
    {
      "id": "title",
      "label": "Title",
      "type": "text"
    },
    {
      "id": "created_at",
      "label": "Created At",
      "type": "date"
    },
    {
      "id": "created_by",
      "label": "Created By",
      "type": "text"
    }
  ]
}

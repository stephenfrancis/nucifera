{
  "_id": "design",
  "template": "builtin:view",
  "description": "View of all design documents for a database - views and templates",
  "type": "list",
  "index": ["template"],
  "selector": {
    "template": {
      "$regex": "^builtin:"
    }
  },
  "columns": [
    {
      "field": {
        "id": "_id",
        "type": "text"
      },
      "label": "Title"
    },
    {
      "field": {
        "id": "template",
        "type": "text"
      },
      "label": "Template"
    },
    {
      "field": {
        "id": "type",
        "type": "text"
      },
      "label": "Type"
    }
  ]
}

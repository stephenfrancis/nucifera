{
  "_id": "deciduouses",
  "template": "builtin:view",
  "type": "list",
  "index": ["template"],
  "selector": {
    "template": {
      "$eq": "deciduous"
    }
  },
  "columns": [
    {
      "field": {
        "id": "name",
        "type": "text"
      },
      "label": "Name"
    },
    {
      "field": {
        "id": "created_at",
        "type": "date"
      },
      "label": "Created At"
    },
    {
      "field": {
        "id": "when_leaves_fall",
        "type": "text"
      },
      "label": "When Leaves Fall"
    }
  ]
}

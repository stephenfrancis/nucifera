{
  "_id": "shrubs",
  "template": "builtin:view",
  "type": "list",
  "index": ["template"],
  "selector": {
    "template": {
      "$eq": "shrub"
    }
  },
  "columns": [
    {
      "field": {
        "id": "name",
        "type": "text"
      },
      "label": "Name"
    },
    {
      "field": {
        "id": "created_at",
        "type": "date"
      },
      "label": "Created At"
    },
    {
      "field": {
        "id": "max_height",
        "type": "number"
      },
      "label": "Max Height"
    }
  ]
}
